AmbulanceScreen:
<AmbulanceScreen>:
    name :"AmbScreen"
    canvas.before:
        Color:

           # rgb: 0.2, 0.5, 1, 0.01
        Rectangle:
            size :self.size
            pos : self.pos
    MDBoxLayout:
        orientation: 'vertical'

        padding: 10
        spacing:10
        MDBoxLayout:
            orientation: 'vertical'
            size_hint_y:-1
            MDFloatLayout:
                default_size_hint:1,None
                size_hint_y:0.1
                MDIconButton:
                    icon:"arrow-left"
                    pos_hint:{'center_y':.91}
                    user_font_size : "20sp"
                    theme_text_color: "Custom"
                    text_color:rgba(71,92,119,255)
                    on_release:
                        root.manager.current='home'
                MDIconButton:
                    icon:"dots-vertical"
                    pos_hint:{"center_x":.93,'center_y':.94}
                    user_font_size : "20sp"
                    theme_text_color: "Custom"
                    text_color:rgba(71,92,119,255)
                    on_release:
                        print("Settings")
                MDLabel:
                    text:"Ambulance Service"
                    font_name: "BPoppins"
                    font_size: "20sp"
                    pos_hint:{"center_x":.79,"center_y":.90}
                    color:rgba(71,92,119,255)

            MDCard:
                orientation: 'vertical'
                md_bg_color:0.9,1,0.9,0.6
                size_hint:1, None
                pos_hint:{'center_x':.5,'center_y':.5}
                height: dp(70)
              #  elevation: 5
                radius: 20
                #spacing:10
                MDTextField:
                    id : textfield
                    hint_text: "Search"
                    icon_right: 'magnify'
                    on_text: root.search()

                    size_hint_x:.8
                    pos_hint:{'center_x':.5,'center_y':.5}
